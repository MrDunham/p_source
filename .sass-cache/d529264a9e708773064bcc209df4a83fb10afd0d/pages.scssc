$blue_lt: #93a7bb; 
$blue_med: #567;
$blue_dark: #414a59;

$white: #f8f8f8;

/****************************************************************/
/*******************     Applies to all      ********************/
/****************************************************************/
#signup_header{
    text-align: center;
    padding-bottom: 40px;
}

#middleInfoInner{
    h2, h3{font-weight: 300;}
    .data{
        padding-bottom: 4rem;
        
        h3{
            text-align: center;
            & + p{ text-align: center;}
        }
    }
    
    &>div.middle_headline{
        text-align: center;
        padding: 4rem 0 1.5rem;
        font-size: 1.125em;
        
    }
    h2, h3{
            font-weight: 300;
            
            & + p{
                font-size: 1.3rem;
                font-size: 1.125em;
                line-height: 1.3em;
                color: #777;
            }
        
        }
    
     
    .button{
        margin-bottom: 0;
        padding: 0.5625rem 2rem 0.5rem;
        background-color: $blue_med;
        &:hover{border-bottom: none;}
    }
    
    /*&>div:last-child{   //not needed if doing all "data" elements have a built-in padding-bottom
        padding-bottom: 4rem;
    }*/
}

p.quote{
    padding: 2rem;
    background: #fff;
    
    display: inline-block;
    padding: 1rem;
    
    span{
        float: right;
    }
}

/****************************************************************/
/*******************        More Info        ********************/
/****************************************************************/
#list_sponsors{
    float:right;
}
.listed_sponsor{
    margin: 20px 0;
}
.listed_sponsor a{
    border-bottom: none;
}
.listed_sponsor img{
    max-width: 160px;
}

#homepage_about{
    #middleInfoInner{
        h3{
            text-align: left;
            
            
            & + p{
                font-size: 1rem;
                color: #000;
                text-align: left;
            }    
        }
    }
}

/****************************************************************/
/*******************     Home_Accelerate     ********************/
/****************************************************************/
#middleInfoInner{
    #industry_partners, #investors{
        li{
            text-align: center;
            height: 5rem;
            line-height: 5rem;
        
            img{
                max-height: 4rem;
                max-width: 9rem;
                vertical-align: middle;
                &.partner_company{max-height: 3rem; max-width: 12rem}
                
            }
        }
    }
    
    
    .program_grid{
        & > div{
            margin-bottom: 1rem;
            padding: 0.825rem;
            background: $blue_med;
            & > p{
                margin-bottom: 0;
                color: #fff;
                
                &:last-child{
                    text-align: right;
                }
            }
            
        
        }            
    }
    
    #industry_partners{}
    #investors{
        text-align: center;
    }
}


/****************************************************************/
/*******************  Attendee registration  ********************/
/****************************************************************/

#register_buttons{
    margin: 20px 0 40px;
}
#register_or p{
    font-size: 28px;
    padding-top:10px;
}
#register_buttons .register_button {
    border-bottom:none !important;
}
#container, html.embed {
    border-radius: 6px;
}
#header.info {
    background: #409DBE;
    padding-top: 10px;
    border-radius: 6px 6px 0 0;
}
form.wufoo.page1 {
    padding: 10px;
}
#have_invite, #need_invite {}
#eventbrite{}

#invite{
    padding: 10px;
    background: rgb(133,133,133);
    background: rgba(0,0,0,.3);
}

.important_list h2, .important_list h4{
    text-align: center;
}

.important_list h2{
    padding-bottom: 10px;
}

.important_list h4{
    padding-top: 10px;
}

.important_list li{
    color: white;
    font-size: 20px;
    margin: 10px 15px;
    padding: 10px;
    background: rgba(12, 12, 12, .13);
    text-align: center;
}



/****************************************************************/
/*******************        Sponsors         ********************/
/****************************************************************/
#sponsorships_page{
    padding: 40px 0 20px;
}
.sponsorships_info {
    margin-bottom: 50px;
}
.sponsorships_info h1, .sponsorships_info h2{
    margin-bottom: 10px;
}

/*.sponsorships_info h2{*/
/*    margin-top:40px;*/
/*}*/

.active .sponsor_grid{
    background: #dEd9bD;
    /*background: rgba(100,100,100,0.2);*/
    transition: background 0.4s;
    -moz-transition:background 0.4s; /* Firefox 4 */
    -webkit-transition:background 0.4s; /* Safari and Chrome */
    -o-transition:background 0.4s; /* Opera */
}
.sponsor_grid{
    background: #CEC9AD;
    padding: 10px;
    /*border-radius: 6px;
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    -o-border-radius: 6px;*/
}

.sponsor_grid h2{
    border-bottom: solid #f5f5f5 1px;
    margin-bottom: 10px;
    font-size: 14px;
}
.sponsor_grid h3{
    font-size: 22px;
}
.sponsor_grid p{}
.sponsor_grid .link{
    margin-top: 20px;
    color: #f5f5f5;
}
#sponsor_bottom{
    /*background: #EEEEEE url("/images/bg_linen_dark.png") repeat;*/
    border-top: 2px solid #e2e5cf;
}
.more_sponsor{
    margin: 30px 0 30px 5px;
    padding: 10px;
    background: #CEC9AD;
    min-height:400px;
    min-width: 620px;
}
.learn_more_inner ul li{
    padding-left: 20px;
    list-style-type: disc;
    list-style-position: inside;
}
.faq_answer .negspace{
    top: -60px;
}


/***************** Large Banner ****************/

lg_banner.left, .lg_banner_right{
    margin: 0 auto 0;
    line-height: 130px;
    width: 845px;
}
.lg_banner.left{
    text-align: right;
}
.lg_banner.left:before, .lg_banner.right:after {
    content: "";
    float: left;
    display: block;
    width: 0;
    height: 0;
    border-bottom: 65px solid #c1c1c1;
    border-top: 65px solid #c1c1c1;
}
.lg_banner.left:before {
    border-right: 15px solid #c1c1c1;
    border-left: 65px solid transparent;
    margin-right: -5px;
}
.lg_banner.left::after {
    border: 2px dotted rgba(15, 15, 15, 0.8);
    content: "";
    float: left;
    display: block;
    line-height: 134px;
    height: 131px;
    top: -3px;
    position: relative;
    left: -2px;
    z-index: 3;
}
.lg_banner.right::before {
    border: 5px solid rgba(25, 25, 25, 1);
    content: "";
    float: left;
    display: block;
    line-height: 134px;
    height: 131px;
    top: -5px;
    position: relative;
    left: 5px;
    z-index: 1;
    border-radius: 6px;
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    -o-border-radius: 6px;
}
.lg_banner.right:after {
    border-right: 65px solid transparent;
    border-left: 15px solid #c1c1c1;
    margin-left: -5px;
}



/*box-shadow: -4px 0 5px -1px rgba(195,195,195,.6);*/
.lg_banner_content{
    display: block;
    background: #c1c1c1;
    float: left;
    padding: 6px 20px;
    position: relative;
    z-index: 2;
}
.lg_banner_content a, .lg_banner_content h2{
    display: inline-block;
    position: relative;
}

.lg_banner h1{
    color: #151515;
}
.lg_banner p{
    color:#252525;
    margin-bottom: 5px
}


/****************************************************************/
/*******************        Ignition         ********************/
/****************************************************************/
.color_scheme_blue{
    h1,h2,h3,h4,p, ul li, ul, #links li a{
        color: #000;
        text-shadow: 0 1px rgba(255,255,255,0.1);
    }
}

#event_home{
    > h4, > p{
        display: inline-block
    }
    h4{
        width: 120px;
        float: left;
    }
    > p{
        width: 85%;
        margin-bottom: 15px;
    }
    
    #programs{
        margin-bottom: 15px;
    }
}




/****************************************************************/
/*******************            FAQ          ********************/
/****************************************************************/
.faq_col.top{
    margin-top:0px;
}
.faq_col{
    margin-top:25px;
}
.faq_col li{
    margin-left:10px;
}

#faq_answers > h3, #faq_answers > h4{
    margin-top: 30px;
    margin-left: 10px;
}
.faq_answer.top{
    margin-top: 0;
}

.faq_answer{
    margin-top: 20px;
    padding: 10px;
    background: rgba(0,0,0,.07);
    border-radius: 6px;
}

#new_faq .faq_col li{
    margin-left: 0;
    margin-top: 10px;
}


#new_faq .faq_answer{
    margin-top: 0;
    padding: 0;
    background: transparent;
    border-radius: 0;
}
.totop{
}

/****************************************************************/
/************  Conversations / speaker panel event  *************/
/****************************************************************/
.boxed_info{
    box-sizing: border-box;

    & > p{
        padding: 3px 10px;
        background-color: $blue_med;
        color: $white;
    }
}

.speaker_list{
    span{
        background: $blue_med;
        /*top: -4px;*/
        padding-top: 5px;
        padding-bottom: 5px;
    }
    p:first-child{
        margin: 0.875rem 0;
    }
    p{
        color: $white;
        line-height: 1.2;
    }
}

#upcoming_event{
    padding: 2em 0 2.5rem;
    
    .event_info{
        font-size: 1.15rem;
    }
}

#stay_connected{
    
    background: rgb(228,230,230);
    
    #stay_connected_inner{
        padding: 2.5rem 0;
    }
    
    form, p, .button{margin-bottom: 0;}
    
    #connected_text{}
    #connected_form{
        margin-top: 1rem;
        
        .button{
            padding: 0.5625rem 2rem 0.5rem;
            background-color: $blue_med;
        }
    }
}



/********   Size specific   ********/

@media (max-width: 40em){
        #header_text{
            
            &>:first-child{padding-top: 0rem;}
            &>:last-child{padding-bottom: 0.8rem;}
            
            h1{
                font-size: 1.5rem;
                font-weight: 300;
            }
            
            p {font-size: 1.2rem;}
        }
        
        .speaker_list{
            h3{font-size: 1rem}
            p {font-size: 0.8rem}
        }
        
        .boxed_info{
            p{font-size: 1rem;}
        }
    }
    @media (min-width: 40.063em){
        #header_text{
            &>:first-child{padding-top: 60px;}
            &>:last-child{padding-bottom: 40px;}
        }
       
        .speaker_list{
            h3{font-size: 1rem}
            p {font-size: 0.8rem}
        }
         
         .boxed_info{
            p{font-size: 1.4rem;}
         }
    }
    @media (min-width: 64.063em){
        #header_text{
            &>:first-child{padding-top: 3rem;}
            &>:last-child{padding-bottom: 2.9rem;}
        
        }
       
       .speaker_list{
            p:first-child{font-size: 1.2rem}
            /*p {font-size: 1rem}*/
        }
        
        .boxed_info{
            p{font-size: 1.6rem;}
        }
    }




