/********* mentors   *********/


$main_bg_color: #202740;
$mid_bg_color: #37467d;
$lt_bg_color: #B1BEEF;

$text_color: #ffffff;
$main_z: 15;

$white: #f5f5f5;
$black: #000000;
$green: #1e8b36;

$spacer: 10px;


$tag_color: #fff;

#mentors{
    
    
    #mentors_inner{
        .mentor{
            position: relative;
            padding:5px;
            background: #fff;
            box-shadow: #666666 0px 1px 1px;
            margin-bottom: 10px;
            @include box_sizing;
            p {
               margin-bottom: 0;
            }
            img{
                width:100%;
                
            }
            span{
                position: absolute;
                bottom: 5px;
                left: 0px;
                padding: 5px 10px;
                @include modal_bg_blk;
                p{
                    color: $white;
                }
                .mentor_name{
                    font-size: 20px;
                }
                .mentor_employer{
                    font-size: 14px;
                }
            }
        }
        
        .full_mentor_details{
            margin-bottom: 20px;
            min-width: 100%;
            img{
                background: #fff;
                box-shadow: #666666 0px 1px 1px;
                padding:5px;
            }
            .mentor_details_info{
                .name{
                font-size: 36px;
                }
                .bio{
                    font-size: 16px;
                }
                
                h5{
                    margin-top: 20px;
                    font-size: 24px;
                }
                
                $tag_dist: 16px; //Sets the tag height, width, and borders.
                .tags{
                    margin-top: 10px;
                    .tag_wrap{
                        margin-left: 5px;
                        position: relative;
                        display: inline-block;
                        .tag{
                            padding: $tag_dist - 8;
                            padding-left: 8px;
                            display: inline-block;
                            background: #fff;
                            box-shadow: #666666 0px 1px 1px;
                            color:#000;
                            text-shadow: none;
                            @include box-shadow;
                            margin-bottom: 5px;
                        }
                 
                    }
                }
            }
            
            
            
            

        }
    }
}


