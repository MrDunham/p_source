@import "standard-import";
@import "mc";

/*Mixins*/
@import "standard-mixins";


/*Variables*/
$white: #f8f8f8;
$blue_lt: #93a7bb; //#3792b6
$blue_med: #567; //#006295
$blue_dark: #414a59; // #465059; //#005375

$blue_med_overlay: rgba(85, 102, 119, 0.4);

/************************* Layout *************************/
h1, h2, h3, h4, h5{
    font-weight: 100;
}

@media only screen and (max-device-width: 480px){
    font-weight: 300;
}

p, div, span, a{
    font-weight: 300;
}


.piclink {
border-bottom: none !important;
}

#homepage_landing{#upperInfoContainer{min-height: 42rem;}}
@media (max-width: 40em){#homepage_landing{#upperInfoContainer{min-height: 0rem !important;}}}

#homepage_ignition{
    #upperInfoContainer{
        /*background-image: url("../images/bgs/ignition_home_header.jpg");*/
        /*background-image: url("../images/bgs/ignition_room.jpg");*/
        overflow: hidden;
        background-repeat: no-repeat;
        -webkit-box-shadow: 0 1px 1px #999999;
        -moz-box-shadow: 0 1px 1px #999999;
        box-shadow: 0 1px 1px #999999;
        background-size: 100%;
        background-position: top center;
    }
    
}

/************* Upper info *************/
    
/*******Color schemes ******/
#upperInfoContainer{
    
    
    background-repeat: repeat;
    position: relative;
    @include box-shadow(0 1px 1px #999);
    
    &.color_scheme_default, .color_scheme_backup{background-color: $blue_med;}
    
}




#headerImg{
    /*background: url('../images/headerimg.jpg');*/
    background-size: cover;
    position:absolute;
    text-align: center;
    width:100%;
    min-height: 100%;
    bottom: 0;
    opacity:0.1;
    filter:alpha(opacity=10); /* For IE8 and earlier */
    z-index:0;
}

#upperInfoInner{
    position:relative;
    z-index:2;
    
    #linkBar, #sub_link_bar{
        
        background-color: rgba(255, 255, 255, 0.55);
        padding: 20px 0;
        position: relative;
        
        a.menu-icon:hover, a{
            border-bottom: none;
        }
        
        a.piclink:active{background: none;}
        
        nav{
            
            $nav_bg: rgba(255,255,255,.6);
            margin-top: -0.2rem;
            display: inline-block;
            /*text-align: right;*/
            float: right;
            ul{
                margin-bottom: 0;
                font-size: 1.3rem;
                
                a{
                    display: inline-block;
                }
                li{ // Main links (always desplayed / top line, etc.)
                    display: inline-block;
                    
                    a{
                        color: #333;
                        padding: 0.6rem 1rem;
                        border-bottom: none;
                        
                        &.showme{
                            background: $nav_bg;
                        }
                    }

                }
                ul { // Sub links
                    display: none;
                    &.box{
                        padding: 10px;
                        background: $nav_bg;
                    }
                    li{
                        display: block;
                        float: none;
                        &:after{
                            content:'';
                        }
                        a{
                            padding: 0.4rem 0;
                        }
                    }
                }
                li{
                    &:hover, a:focus, &.active{
                        background: $nav_bg;
                    }
                    &:hover> ul, a:focus + ul, .box.showme{
                        display: block;
                        position: absolute;
                        text-align: left;
                        float: left;
                    }
                }
            }
        }
        
        @media (max-width: 40em){
            background: none;
            .logoarea {
                text-align: left;
                padding-left: 3rem;
            }
            nav{text-align: center}
        }
        @media (min-width: 40.063em){
           .logoarea {
            text-align: center;
            }
            nav{text-align: center}
        }
        @media (min-width: 64.063em){
           .logoarea {
                text-align: left !important;
            }
            nav{text-align: right !important}
        }
    }
    
    #sub_link_bar{
        background: rgba(65,74,89,.55);
        padding: 0;
        
        h2{
            color: $white;
            
        }
        nav{
            margin-top: 0.2rem;
            ul{
                li{
                    margin-top: 0.7rem;
                    a{
                        color: $white;
                        padding: 0.1rem 0.8rem;
                    }
                    a:hover{color:#000;}
                    &.active{a{color:#000;}}
                }
            }
        }
    }
}



#homepageLogo{
    display: inline-block;
}





.box{
    background: rgba(255,255,255,.55);
    color: white;
    padding: 20px;
    h1{
        font-size: 42px;
        text-shadow: none;
        font-weight: 100;
    }
}


